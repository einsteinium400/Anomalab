#:kivy 2.1

<RegularResults>:
    BoxLayout:
        orientation: 'vertical'
        padding: [10,50,10,50]
        spacing: 50

        Image:
            source: 'gui/images/logo.jpg'

        Label:
            text: "Here come the results"
            font_size: 32
            
        GridLayout:
            size: root.width - 10, root.height -10
            cols: 2
            Button:
                text: 'Another Query'
                font_size: 24
                on_press: root.on_apply()
            Button:
                text: 'Choose Dataset'
                font_size: 24
                on_press: root.on_back()

        
